{
  "name": "@nuxtjs/google-adsense",
  "version": "1.4.0",
  "description": "Google Adsense Module for Nuxt.js",
  "license": "MIT",
  "repository": "https://github.com/nuxt-community/adsense-module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --ext .vue,.ts,.js .",
    "test:nuxt2": "FIXTURE_NAME=nuxt2 mocha --timeout 10000 --exit -r jiti/register test/**/*.test.ts",
    "test:bridge": "FIXTURE_NAME=nuxt2 mocha --timeout 10000 --exit -r jiti/register test/**/*.test.ts",
    "test:nuxt3": "FIXTURE_NAME=nuxt2 mocha --timeout 10000 --exit -r jiti/register test/**/*.test.ts",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "pathe": "^0.2.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^0.2.2",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.5",
    "jsdom": "^19.0.0",
    "mocha": "^9.1.3",
    "nuxt": "^2.15.8",
    "playwright": "^1.17.1",
    "standard-version": "^9.3.2",
    "unbuild": "^0.6.7"
  },
  "keywords": [
    "AdSense",
    "Google",
    "Nuxt",
    "NuxtJS",
    "Nuxt.js",
    "PWA",
    "SSR",
    "Vue",
    "VueJS",
    "Vue2",
    "VueRouter",
    "vue-router",
    "Web"
  ]
}
